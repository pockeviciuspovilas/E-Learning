@model List<Elearn.Models.Test>

@{
    Layout = "~/Views/Report/Shared/_Layout.cshtml";
    ViewData["Title"] = "Mano testai";
    
}
<h2> Kategorijos ataskaita</h2>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Testo pavadinimas</th>
      <th scope="col">Užduota</th>
      <th scope="col">Spręsta</th>
      <th scope="col">Pažymių vidurkis</th>
    </tr>
  </thead>
  <tbody>
@for(int i=0;i < Model.Count; i++)
{
    @:
    <tr>
    <th scope="row">@Model[i].Name</th>
    <td>@ViewBag.participantCountList[i]</td>
    <td>@ViewBag.completedCount[i]</td>
    <td>@ViewBag.averageList[i]</td>
    </tr>

} 
  </tbody>
</table>

